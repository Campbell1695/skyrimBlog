<%= form_for(@user, url: user_path) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :email %>
<%= f.email_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.submit "start", class: "btn btn-primary" %>
<% end %>

<%= link_to 'Back', user_path(current_user)%>
